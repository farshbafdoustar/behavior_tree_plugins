<?xml version="1.0"?>
<package format="2">
  <name>behavior_tree_plugin_ros</name>
  <version>0.1.0</version>
  <description>The BT plugin that calls any ROS Service or Action</description>


  <maintainer email="Mahmoud@Farshbaf.Net">Mahmoud FArshbaf</maintainer>

  <license>MIT</license>


  <!-- Url tags are optional, but multiple are allowed, one per tag -->
